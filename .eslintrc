{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "max-len": 0,
    "no-console": 0,
    "no-nested-ternary": 0,
    "linebreak-style": "off",
    "arrow-parens": "off",
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [ "Link" ],
      "specialLink": [ "to" ]
    }],
    "global-require": "warn",
    "import/no-extraneous-dependencies": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "jsx-a11y/href-no-hash": "off",
    "react/prop-types": 0
  },
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  }
}
