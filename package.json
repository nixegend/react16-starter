    "react": "^16.8.6",
    "react-app-polyfill": "^0.2.2",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.3",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "react-router-redux": "^4.0.8",
    "react-transition-group": "^2.9.0",
    "redux": "^4.0.1",
    "redux-api-middleware": "^3.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "uuid": "^3.3.2"
  },
