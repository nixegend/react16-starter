{
  "name": "react_16_starter",
  "version": "1.0.0",
  "description": "react16 starter",
  "main": "./server/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./dev-starter.js --color",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --color",
    "test": "jest",
    "test:w": "npm t -- -w",
    "test:c": "npm t -- --coverage",
    "testa": "mocha --require babel-register --require jsdom-global/register --require babel-polyfill --require ignore-styles \"src/**/*.test.js\" --color --require ./tests/setup.js",
    "testsss": "mocha --require babel-register --require jsdom-global/register --require babel-polyfill --require ignore-styles \"src/**/*.test.js\" -c --require tests/setup",
    "test-team-city": "mocha --reporter mocha-teamcity-reporter --require babel-register --require jsdom-global/register --require babel-polyfill --require mock-css-modules \"src/**/*.test.js\" -c --require tests/setup",
    "coverer": "node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --require jsdom-global/register --require babel-polyfill --require mock-css-modules \"src/**/*.test.js\" -c --require tests/setup",
    "clean": "rimraf build",
    "flow": "./node_modules/.bin/flow",
    "winflow": "flow check",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint src --ext .js --fix --color",
    "lint:scss": "stylelint \"./src/**/*.scss\" --config .stylelintrc --fix --color"
  },
  "author": "Anton Panchenko <nixegend@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-history-api-fallback": "^2.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-api-middleware": "^2.2.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-istanbul": "^0.12.2",
    "babel-jest": "^22.4.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "expect": "^22.2.2",
    "expect-enzyme": "^1.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.65.0",
    "flux-standard-action": "^2.0.1",
    "html-webpack-plugin": "^2.30.1",
    "ignore-styles": "^5.0.1",
    "jest": "^22.3.0",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.0",
    "nock": "^9.1.6",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.12",
    "open": "^0.0.5",
    "postcss": "^6.0.17",
    "postcss-loader": "^2.1.0",
    "precss": "^3.1.0",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.1",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.2",
    "style-loader": "^0.20.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^2.3.0",
    "stylelint-webpack-plugin": "^0.10.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nixegend/react16-starter.git"
  },
  "keywords": [
    "react16",
    "starter"
  ],
  "bugs": {
    "url": "https://github.com/nixegend/react16-starter/issues"
  },
  "homepage": "https://github.com/nixegend/react16-starter#readme",
  "jest": {
    "moduleFileExtensions": ["js"],
    "moduleDirectories": ["node_modules"],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ],

    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/tests/mocks/styleMock.js"
    }
  }
}
