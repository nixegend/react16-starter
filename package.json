{
  "name": "react_starter",
  "version": "1.0.0",
  "description": "react starter web application",
  "scripts": {
    "start": "cross-env NODE_ENV=development VM_TYPE=dev webpack-dev-server --hot --open --color",
    "build:prod": "cross-env NODE_ENV=production VM_TYPE=prod webpack --progress --color",
    "build:stage": "cross-env NODE_ENV=production VM_TYPE=stage webpack --progress --color",
    "build:qa": "cross-env NODE_ENV=production VM_TYPE=qa webpack --progress --color",
    "test": "jest --color",
    "test:watch": "jest --watch \"./src/**/*.test.js\" --color",
    "test:update": "jest --updateSnapshot --color",
    "test:coverage": "jest --coverage --color",
    "lint:js": "eslint \"./src/**/*.js\" --config .eslintrc --fix --color",
    "lint:scss": "stylelint \"./src/**/*.scss\" --config .stylelintrc --fix --color"
  },
  "pre-commit": [
    "test",
    "lint:js",
    "lint:scss"
  ],
  "license": "Commercial",
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "dependencies": {
