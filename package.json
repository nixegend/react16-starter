{
  "name": "react_16_starter",
  "version": "1.0.0",
  "description": "react16 starter",
  "main": "./server/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./dev-starter.js --color",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --color",
    "test": "jest",
    "test:w": "jest --watch",
    "test:c": "jest --coverage",
    "clean": "rimraf build",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint src --ext .js --fix --color",
    "lint:scss": "stylelint \"./src/**/*.scss\" --config .stylelintrc --fix --color"
  },
  "author": "Anton Panchenko <nixegend@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-history-api-fallback": "^2.2.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-api-middleware": "^2.3.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-mock": "^6.3.0",
    "file-loader": "^1.1.11",
    "flux-standard-action": "^2.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.3",
    "open": "0.0.5",
    "postcss": "^6.0.21",
    "postcss-loader": "^2.1.4",
    "redux-mock-store": "^1.5.1",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.1.2",
    "react-test-renderer": "^16.3.2",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "sass-resources-loader": "^1.3.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.1.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nixegend/react16-starter.git"
  },
  "keywords": [
    "react16",
    "starter"
  ],
  "bugs": {
    "url": "https://github.com/nixegend/react16-starter/issues"
  },
  "homepage": "https://github.com/nixegend/react16-starter#readme",
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/tests/mocks/styleMock.js"
    }
  }
}
